


      <%= form_with(model: contato, local: true) do |form| %>
        <% if contato.errors.any? %>
          <div id="error_explanation">
            <h2><%= pluralize(contato.errors.count, "error") %> prohibited this contato from being saved:</h2>

            <ul>
            <% contato.errors.full_messages.each do |message| %>
              <li><%= message %></li>
            <% end %>
            </ul>
          </div>
        <% end %>


      <div class="form-group">
        <label class="col-md-4 control-label" for="formGroupInputLarge">Nome</label>
        <div class="col-xs-8">
            <%= form.text_field :nome, id: :contato_nome, class: 'form-control' %>
        </div>
      </div>
      
      <div class="form-group">
        <label class="col-md-4 control-label" for="formGroupInputLarge">Endereço</label>
          <div class="col-xs-8">
            <%= form.text_field :endereco, id: :contato_endereco, class: 'form-control' %>
          </div>
        </div>

      <div class="form-group">
        <label class="col-md-6 control-label" for="formGroupInputLarge">E-Mail</label>
          <div class="col-xs-8">
            <%= form.text_field :email, id: :contato_email, class: 'form-control' %>
          </div>
      </div>

      <div class="form-group">
          <label class="col-md-12 control-label" for="formGroupInputLarge">Data Nascimento</label>
          <div class="col-xs-8">
          <%= form.date_select :dataNascimento, id: :contato_dataNascimento, start_year: 1950, order: [:day, :month, :year], :use_month_names=>["Janeiro" ,"Fevereiro", "Março", "Abril" ,"Maio", "Junho","Julho", "Agosto", "Setembro", "Outubro", "Novembro", "Dezembro"],  class: 'selectpicker'%>
         </div>
        </div>
    
    
  
      <div class="text-center">
        <div class="actions">
          <%= form.submit class: 'btn btn-success center-block'%>
          <%= link_to 'Voltar', contatos_path, class: "btn btn-link" %>
        </div>
      </div>
  <% end %>
 